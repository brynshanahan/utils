{
  "name": "@eddev/utils",
  "version": "1.0.0",
  "dependencies": {
    "@juggle/resize-observer": "^3.1.3",
    "intersection-observer": "^0.8.0"
  },
  "types": "./lib/@types",
  "scripts": {
    "dev": "npx concurrently 'yarn compile-code' 'yarn compile-types'",
    "prod": "npx parcel build src/*/*.ts --out-dir ./lib && tsc",
    "compile-types": "node ./scripts/run-tsc.js",
    "compile-code": "npx parcel watch src/*/*.ts --out-dir ./lib"
  },
  "peerDependencies": {
    "@juggle/resize-observer": "^3.1.3",
    "intersection-observer": "^0.8.0"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "parcel": "^1.12.4",
    "typescript": "^3.8.3"
  }
}
